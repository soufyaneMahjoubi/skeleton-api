{
    "parser": "espree",
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "import/newline-after-import": [
        "off"
      ],
      "no-nested-ternary": [
        "error"
      ],
      "no-unneeded-ternary": [
        "error"
      ],
      "no-var": [
        "error"
      ],
      "brace-style": [
        0,
        "1tbs"
      ],
      "indent": [
        "error",
        4
      ],
      "one-var": [
        "error",
        "never"
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": false
        }
      ],
      "space-infix-ops": [
        "error"
      ],
      "func-names": [
        "warn",
        "as-needed"
      ],
      "class-methods-use-this": [
        "off"
      ]
    },
    "globals": {
      "expect": true,
      "$": true,
      "document": true
    }
  }
  